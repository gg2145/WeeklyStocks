# Weekly Stocks Configuration
# Copy this file to .env and customize as needed

# Comma-separated list of stock tickers
TICKERS=AAPL,MSFT,GOOGL,AMZN,TSLA,META,NVDA,NFLX,CRM,ADBE

# Start date for data fetching (YYYY-MM-DD format)
# Leave empty to use default (3 months ago)
START_DATE=

# Output directory (relative to project root)
OUTPUT_DIR=output

# Log level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# Maximum retries for failed API calls
MAX_RETRIES=3

# Delay between retries (seconds)
RETRY_DELAY=1
